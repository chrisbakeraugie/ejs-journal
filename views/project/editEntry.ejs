<form action="/projects/<%= entry._id %>/edit?_method=PUT" method="POST">
  <label for="title">Title</label>
  <input type="text" name="title" value="<%= entry.title %>">
  <label for="description"></label>
  <input type="text" name="description" value="<%= entry.description %>">
  <button class="btn btn-light" type="submit">Update</button>
</form>
