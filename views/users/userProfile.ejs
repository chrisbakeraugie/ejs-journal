<h2><%= currentUser.fullName %></h2>

<h3>Your information:</h3>
<p>Name: <%= currentUser.name.first %> <%= currentUser.name.middle %> <%= currentUser.name.last %></p>
<p>Joined: <%= currentUser.createdAt %></p>
<p></p>
<% if (currentUser.projects.length > 0) { %>
<h4>You have <%= projects.length %> projects</h5>
  <% projects.forEach(project => { %>
  <h6><a href="/projects/<%= project._id %>"><%= project.title %></a></h6>
  <% })} %>